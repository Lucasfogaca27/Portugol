programa
{
		
	inteiro i = 0
	inteiro valor, resultado = 0
	
	funcao inicio()
	{
		para(i=0; i <5; i++){
			escreva("Digite um valor: \n")
			leia (valor)
			resultado += valor
		}

	escreva ("A soma dos 5 valores é: " +resultado)	
	
	}
}
