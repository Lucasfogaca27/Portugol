programa
{
	
	inteiro v[6] 
	inteiro i
	
	funcao inicio()
	{
		para(i=0; i<=5; i++){
			escreva("Digite os valores do Vetor: \n")
			leia(v[i])
		}
		para(i=0; i<=5; i++){
			escreva("\nDigite os valores do Vetor: \n", v[i])
			
		}
	}
}

